#!/bin/bash

## -----------------------------------------------------------------------
#  nome do computador
#
HOST=`hostname`

## -----------------------------------------------------------------------
#  diretorio da aplicacao
#
cd ~/SiPAR/atual

## -----------------------------------------------------------------------
#  executa a aplicacao
#
/usr/bin/env python -OO ./pwiz.py 2> ./piloto.$HOST.out

## -----------------------------------------------------------------------
#  diretorio raiz
#
cd ~
